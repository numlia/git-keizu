{
	"compilerOptions": {
		"lib": [
			"es2020",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "../media",
		"removeComments": true,
		"strict": true,
		"target": "es5"
	}
}