{
  "compilerOptions": {
    "declaration": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "outDir": "../out",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6"
  }
}
